    "test:watch": "vitest",
    "lint": "eslint . --ext .ts",
    "format": "prettier . --write",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "prisma": "5.10.0",
    "typescript": "^5.3.3",
    "vitest": "^1.3.1"
  },
  "dependencies": {
    "@prisma/client": "5.10.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "tsx": "^4.7.0"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
